function dash() {
  open dash://$1
}

alias qw='qw -e vim'

# fasd aliases
alias v='f -e vim'

function gh() {
  open "https://github.com/search?q=$1&ref=commandline"
}

function dg(){
  open "https://duckduckgo.com/?q=$1"
}

function so(){
  open "http://stackoverflow.com/?q=$1"
}

